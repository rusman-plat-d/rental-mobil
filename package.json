{
	"name": "pp2",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"build:prod": "ng build --prod -aot --output-hashing=none && ng build --prod --app server --output-hashing=none",
		"build": "ng build --output-hashing=none && ng build --app server --output-hashing=none",
		"generate-sw": "node generate-sw.js",
		"pp2-ngsw": "ngsw-config dist/public ./src/client/ngsw-config.json",
		"postpp2-ngsw": "cp node_modules/@angular/service-worker/ngsw-worker.js ./dist/public/ngsw-worker.js",
		"gg": "npm run build && npm run tsc-server && npm run generate-sw",
		"start": "node dist/server.js",
		"wp": "webpack --config dist/webpack.server.config.js --progress --colors",
		"tsc": "cd src/server && tsc"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^5.1.1",
		"@angular/cdk": "^5.0.1",
		"@angular/common": "^5.1.1",
		"@angular/compiler": "^5.1.1",
		"@angular/core": "^5.1.1",
		"@angular/flex-layout": "^2.0.0-beta.11",
		"@angular/forms": "^5.1.1",
		"@angular/http": "^5.1.1",
		"@angular/material": "^5.0.1",
		"@angular/platform-browser": "^5.1.1",
		"@angular/platform-browser-dynamic": "^5.1.1",
		"@angular/platform-server": "^5.1.1",
		"@angular/platform-webworker": "^5.1.1",
		"@angular/platform-webworker-dynamic": "^5.1.1",
		"@angular/router": "^5.1.1",
		"@angular/service-worker": "^5.1.1",
		"@nguniversal/common": "^5.0.0-beta.5",
		"@nguniversal/express-engine": "^5.0.0-beta.5",
		"@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
		"angularfire2": "latest",
		"animate.css": "latest",
		"classlist.js": "latest",
		"core-js": "latest",
		"firebase": "latest",
		"font-awesome": "latest",
		"hammerjs": "latest",
		"express": "latest",
		"reflect-metadata": "*",
		"idb-keyval": "latest",
		"intl": "latest",
		"materialize-css": "latest",
		"moment": "latest",
		"preboot": "latest",
		"helmet-csp": "latest",
		"rxjs": "latest",
		"socket.io": "latest",
		"socketio-file-upload": "latest",
		"sse": "latest",
		"ejs": "latest",
		"ts-loader": "latest",
		"web-animations-js": "latest",
		"workbox-build": "latest",
		"workbox-sw": "latest",
		"zone.js": "latest"
	},
	"devDependencies": {
		"@angular/cli": "^1.6.1",
		"@angular/compiler-cli": "^5.1.1",
		"@angular/language-service": "^5.1.1",
		"typescript": "~2.4.2",
		"webpack": "^3.10.0"
	}
}
